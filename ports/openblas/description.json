{
  "git_url": "git@github.com:xianyi/OpenBLAS.git",
  "reuse_build": true,
  "cache_time": 30,
  "use_msys": true,
  "environment_variable": [
    "USE_OPENMP=1",
    "use_libtool=1"
  ],
  "cmake_options": "-DBUILD_WITHOUT_LAPACK=OFF -DBUILD_BFLOAT16=OFF",
  "conditional_cmake_options": {
    "windows": "-DF_COMPILER=gfortran -DNOFORTRAN=on"
  },
  "default_build_script": "c",
  "dependency": [
    "perl_env:__cbuild_most_recent_git_tag",
    "fortran_env:__cbuild_most_recent_git_tag"
  ]
}
