{
  "git_url": "git@github.com:xianyi/OpenBLAS.git",
  "use_mingw64": true,
  "git_branch": "develop",
  "reuse_build": true,
  "cache_time": 90,
  "environment_variable": [
    "CTEST_PARALLEL_LEVEL=1"
  ],
  "cmake_options": "-DBUILD_WITHOUT_LAPACK=OFF -DBUILD_BFLOAT16=ON -DUSE_OPENMP=off -DCMAKE_GNUtoMS=on",
  "main_language": "c",
  "build_languages": [
    "fortran"
  ],
  "dependency": [
    "perl_env",
    "fortran_env"
  ]
}
