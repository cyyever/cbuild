{
  "git_url": "git@github.com:OpenMathLib/OpenBLAS.git",
  "reuse_build": true,
  "cache_time": 90,
  "environment_variable": [
    "CTEST_PARALLEL_LEVEL=3",
    "run_test=1"
  ],
  "new_feature": [
    "ml_lib"
  ],
  "cmake_options": "-DBUILD_WITHOUT_LAPACK=OFF -DUSE_OPENMP=off",
  "conditional_cmake_options": {
    "windows": "-DCMAKE_WINDOWS_EXPORT_ALL_SYMBOLS=on -DUSE_OPENMP=off",
    "macos": "-DCMAKE_Fortran_COMPILER_ID=LLVMFlang"
  },
  "main_language": "c",
  "build_languages": [
    "fortran"
  ],
  "dependency": [
    "fortran_env"
  ]
}
