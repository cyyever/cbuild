{
  "git_url": "git@github.com:cyyever/OpenBLAS.git",
  "remote_git_url": "git@github.com:xianyi/OpenBLAS.git",
  "remote_git_branch": "develop",
  "git_branch": "cmake_fix",
  "reuse_build": true,
  "cache_time": 90,
  "environment_variable": [
    "CTEST_PARALLEL_LEVEL=1"
  ],
  "conditional_environment_variable": {
    "windows": [
      "FFLAGS=/names:lowercase /assume:underscore"
    ]
  },
  "cmake_options": "-DBUILD_WITHOUT_LAPACK=OFF -DBUILD_BFLOAT16=ON -DUSE_OPENMP=off",
  "main_language": "c",
  "build_languages": [
    "fortran"
  ],
  "dependency": [
    "perl_env",
    "fortran_env"
  ]
}
