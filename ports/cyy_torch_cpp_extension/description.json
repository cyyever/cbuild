{
  "git_url": "git@github.com:cyyever/torch_cpp_extension.git",
  "environment_variable": [
    "run_test=1",
    "py_pkg_name=cyy_torch_cpp_extension"
  ],
  "main_language": "c",
  "build_languages": [
    "c",
    "python"
  ],
  "conditional_new_feature": {
    "unix": [
      "use_mold"
    ]
  },
  "cmake_options": "-DDISABLE_RUNTIME_ANALYSIS=on -DCUDA_TOOLKIT_ROOT_DIR=${CUDA_TOOLKIT_ROOT_DIR}",
  "dependency": [
    "pytorch:cyy",
    "cyy_naive_cpp_lib[shared_lib]:master"
  ]
}
