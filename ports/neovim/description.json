{
  "git_url": "git@github.com:neovim/neovim.git",
  "master": {
    "main_language": "c",
    "build_languages": [
      "python"
    ],
    "environment_variable": [
      "VisualStudioEdition=1",
      "cmake_generator=native"
    ],
    "dependency": [
      "libuv:__cbuild_most_recent_git_tag",
      "glog:__cbuild_most_recent_git_tag"
    ],
    "system_package_dependency": {
      "ubuntu": [
        "libgtest-dev",
        "libgmock-dev"
      ],
      "archlinux": [
        "msgpack-c",
        "libluv",
        "tree-sitter",
        "unibilium",
        "libtermkey",
        "libvterm",
        "lua-lpeg",
        "lua-mpack",
        "gmock"
      ]
    }
  }
}
