{
  "all_os": {
    "git_url": "git@github.com:neovim/neovim.git",
    "main_language": "c",
    "build_languages": [
      "python"
    ],
    "environment_variable": [
      "VisualStudioEdition=1",
      "USE_BUNDLED_NVIMQT=off",
      "cmake_generator=native"
    ],
    "dependency": [
      "glog"
    ]
  },
  "system_package_dependency": {
    "ubuntu": [
      "libgtest-dev",
      "libgmock-dev"
    ],
    "macos": [
      "tree-sitter"
    ],
    "archlinux": [
      "lua51",
      "luarocks",
      "msgpack-c",
      "libluv",
      "tree-sitter",
      "unibilium",
      "libtermkey",
      "libvterm",
      "lua-lpeg",
      "lua-mpack",
      "gmock"
    ],
    "freebsd": [
      "editors/neovim"
    ]
  }
}
