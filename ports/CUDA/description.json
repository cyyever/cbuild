{
  "cache_time": 300,
  "conditional_new_feature": {
    "docker": [
      "use_cuda_docker"
    ]
  },
  "__cbuild_most_recent_git_tag": {
    "ubuntu": {
      "script_package": true
    },
    "centos": {
      "script_package": true
    },
    "windows": {
      "group_package": true
    },
    "archlinux": {
      "group_package": true
    },
    "linux": {
      "url": "https://developer.download.nvidia.com/compute/cuda/11.3.1/local_installers/cuda_11.3.1_465.19.01_linux.run",
      "checksum": "no_checksum"
    }
  },
  "system_package_dependency": {
    "windows": [
      "cuda"
    ]
  },
  "conditional_system_package_dependency": {
    "archlinux&&!docker": [
      "cuda"
    ],
    "archlinux&&docker": [
      "cuda=11.3.0-2"
    ]
  },
  "dependency": [
    "cxx_env:master"
  ]
}
