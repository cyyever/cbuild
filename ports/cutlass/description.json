{
  "git_url": "git@github.com:NVIDIA/cutlass.git",
  "cache_time": 30,
  "default_build_script": "c",
  "reuse_build": true,
  "environment_variable": [
    "static_analysis=0",
    "clang_tidy_static_analysis=0",
    "clang_tidy_fix=0"
  ],
  "cmake_options": " -DCUTLASS_NVCC_ARCHS_ENABLED='75' -DCUTLASS_NVCC_ARCHS='75' -DCUTLASS_ENABLE_EXAMPLES=off -DCUTLASS_ENABLE_CUDNN=on -DCUDNN_PATH=${INSTALL_PREFIX}/cuda "
}
