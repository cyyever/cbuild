{
  "git_url": "git@github.com:NVIDIA/cutlass.git",
  "cache_time": 30,
  "main_language": "c",
  "reuse_build": true,
  "environment_variable": [
    "static_analysis=1",
    "slow_static_analysis=1"
  ],
  "new_feature": [
    "ml_lib"
  ],
  "cmake_options": "-DCUTLASS_NVCC_ARCHS=90a -DCUTLASS_ENABLE_EXAMPLES=off -DCUTLASS_ENABLE_CUDNN=on -DBUILD_TESTING=off"
}
