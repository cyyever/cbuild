{
  "group_package": true,
  "conditional_new_feature": {
    "support_cuda": [
      "cuda"
    ]
  },
  "conditional_dependency": {
    "all_os": [
      "ninja",
      "numpy",
      "protobuf[shared_lib]",
      "tbb",
      "libjpeg-turbo",
      "openblas[shared_lib]",
      "libpng:libpng16",
      "eigen",
      "pillow-simd"
    ],
    "windows": [
      "zlib"
    ],
    "cuda": [
      "cudnn"
    ]
  },
  "system_package_dependency": {
    "freebsd": [
      "freetype2",
      "libzmq4"
    ],
    "ubuntu": [
      "libomp-dev",
      "clang"
    ],
    "centos": [
      "libomp-devel"
    ],
    "archlinux": [
      "gperftools",
      "openmp"
    ],
    "pip": [
      "tensorboard"
    ]
  }
}
