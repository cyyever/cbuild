{
  "group_package": true,
  "conditional_new_feature": {
    "support_cuda": [
      "cuda"
    ]
  },
  "conditional_dependency": {
    "all_os": [
      "ninja",
      "protobuf",
      "zlib-ng",
      "openblas"
    ],
    "cuda&&!docker": [
      "cudnn"
    ],
    "cuda&&docker": [
      "cudnn_in_docker"
    ]
  },
  "system_package_dependency": {
    "freebsd": [
      "libzmq4"
    ],
    "ubuntu": [
      "libomp-dev",
      "clang"
    ],
    "centos": [
      "libomp-devel"
    ],
    "archlinux": [
      "gperftools",
      "openmp"
    ],
    "pip": [
      "tensorboard"
    ]
  }
}
