{
  "group_package": true,
  "conditional_new_feature": {
    "support_cuda": [
      "cuda"
    ]
  },
  "conditional_dependency": {
    "all_os": [
      "ninja:__cbuild_most_recent_git_tag",
      "numpy:__cbuild_most_recent_git_tag",
      "tbb:__cbuild_most_recent_git_tag",
      "libjpeg-turbo:__cbuild_most_recent_git_tag",
      "libpng:libpng16",
      "eigen:__cbuild_most_recent_git_tag",
      "pillow-simd:__cbuild_most_recent_git_tag"
    ],
    "windows": [
      "zlib:develop",
      "scipy:__cbuild_most_recent_git_tag",
      "openblas[shared_lib]:win_fortran",
      "protobuf[shared_lib]:master"
    ],
    "unix": [
      "scipy:__cbuild_most_recent_git_tag",
      "openblas[shared_lib]:__cbuild_most_recent_git_tag",
      "protobuf[shared_lib]:__cbuild_most_recent_git_tag"
    ],
    "cuda": [
      "cudnn:__cbuild_most_recent_git_tag"
    ],
    "linux&&cuda": [
      "nccl:__cbuild_most_recent_git_tag"
    ]
  },
  "system_package_dependency": {
    "freebsd": [
      "freetype2",
      "libzmq4"
    ],
    "ubuntu": [
      "libomp-dev",
      "clang"
    ],
    "centos": [
      "libomp-devel"
    ],
    "archlinux": [
      "gperftools",
      "openmp"
    ],
    "pip": [
      "matplotlib",
      "sklearn",
      "pandas",
      "tensorboard"
    ]
  }
}
