{
  "group_package": true,
  "conditional_new_feature": {
    "support_cuda": [
      "cuda"
    ]
  },
  "conditional_dependency": {
    "all_os": [
      "ninja",
      "numpy",
      "protobuf",
      "libjpeg-turbo",
      "zlib-ng",
      "openblas",
      "libpng:libpng16",
      "eigen"
    ],
    "!macos": [
      "pillow-simd"
    ],
    "amd64": [
      "tbb"
    ],
    "cuda": [
      "cudnn"
    ],
    "!amd64": [
      "tbb"
    ],
    "cuda&&!windows": [
      "nccl"
    ]
  },
  "system_package_dependency": {
    "freebsd": [
      "freetype2",
      "libzmq4"
    ],
    "ubuntu": [
      "libomp-dev",
      "clang"
    ],
    "centos": [
      "libomp-devel"
    ],
    "archlinux": [
      "gperftools",
      "openmp"
    ],
    "pip": [
      "tensorboard"
    ]
  }
}
