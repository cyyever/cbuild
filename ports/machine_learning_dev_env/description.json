{
  "group_package": true,
  "conditional_new_feature": {
    "support_cuda": [
      "cuda"
    ]
  },
  "conditional_dependency": {
    "all_os": [
      "ninja",
      "numpy",
      "protobuf[shared_lib]",
      "tbb",
      "libjpeg-turbo",
      "libpng:libpng16",
      "eigen",
      "pillow-simd",
      "scipy"
    ],
    "windows": [
      "zlib:develop"
    ],
    "!linux": [
      "openblas[shared_lib]:cmake_fix"
    ],
    "linux": [
      "openblas_libtool[shared_lib]"
    ],
    "cuda": [
      "cudnn"
    ],
    "linux&&cuda": [
      "nccl"
    ]
  },
  "system_package_dependency": {
    "freebsd": [
      "freetype2",
      "libzmq4"
    ],
    "ubuntu": [
      "libomp-dev",
      "clang"
    ],
    "centos": [
      "libomp-devel"
    ],
    "archlinux": [
      "gperftools",
      "openmp"
    ],
    "pip": [
      "matplotlib",
      "sklearn",
      "pandas",
      "tensorboard"
    ]
  }
}
