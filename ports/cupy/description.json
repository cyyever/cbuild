{
  "git_url": "git@github.com:cupy/cupy.git",
  "cache_time": 30,
  "main_language": "python",
  "dependency": [
    "numpy",
    "cutensor",
    "cuSPARSELt"
  ],
  "environment_variable": [
    "CUPY_NVCC_GENERATE_CODE=current",
    "CUDA_PATH=${CUDA_HOME}",
    "run_test=0"
  ],
  "conditional_new_feature": {
    "unix": [
      "machine_learning_compilers"
    ]
  }
}
