{
  "script_package": true,
  "cache_time": 7,
  "build_languages": [
    "sh"
  ],
  "conditional_system_package_dependency": {
    "!docker": {
      "unix": [
        "curl",
        "git",
        "wget"
      ],
      "linux": [
        "sudo"
      ],
      "freebsd": [
        "sudo"
      ],
      "archlinux": [
        "fakeroot",
        "vim"
      ]
    }
  },
  "system_package_dependency": {
    "archlinux": [
      "jq",
      "which"
    ],
    "bsd": [
      "coreutils",
      "gsed"
    ],
    "windows": [
      "msys2",
      "git"
    ],
    "ubuntu": [
      "lsb-release",
      "software-properties-common",
      "apt-utils",
      "vim-tiny"
    ],
    "centos": [
      "dnf",
      "yum-utils"
    ]
  }
}
