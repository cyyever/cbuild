{
  "group_package": true,
  "system_package_dependency": {
    "windows": [
      "nodejs.install"
    ],
    "archlinux": [
      "nodejs",
      "npm"
    ],
    "fedora": [
      "nodejs"
    ],
    "macos": [
      "nodejs"
    ],
    "freebsd": [
      "www/npm"
    ]
  },
  "conditional_dependency": {
    "ubuntu||centos": [
      "nodejs:__cbuild_most_recent_git_tag"
    ]
  }
}
