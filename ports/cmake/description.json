{
  "git_url": "https://gitlab.kitware.com/cmake/cmake.git",
  "default_build_script": "c",
  "cache_time": 30,
  "reuse_build": true,
  "system_package_dependency": {
    "unix": [
      "cmake"
    ],
    "centos": [
      "openssl-devel"
    ],
    "ubuntu": [
      "libssl-dev"
    ]
  },
  "cmake_options": "-DBUILD_TESTING=OFF",
  "context_environment_variable": {
    "windows": [
      "no_install=1"
    ]
  }
}
