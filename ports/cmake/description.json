{
  "archlinux": {
    "group_package": true
  },
  "windows": {
    "group_package": true
  },
  "all_os": {
    "git_url": "https://gitlab.kitware.com/cmake/cmake.git",
    "main_language": "c",
    "cache_time": 30,
    "reuse_build": true,
    "conditional_dependency": {
      "unix": [
        "openssl"
      ]
    },
    "cmake_options": "-DBUILD_TESTING=OFF",
    "conditional_environment_variable": {
      "windows": [
        "no_install=1"
      ]
    }
  },
  "system_package_dependency": {
    "all_os": [
      "cmake"
    ]
  }
}
