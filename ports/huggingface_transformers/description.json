{
  "git_url": "git@github.com:huggingface/transformers.git",
  "git_branch": "main",
  "main_language": "python",
  "environment_variable": [
    "use_setup_py=1",
    "py_pkg_name=transformers",
    "run_test=0"
  ],
  "conditional_environment_variable": {
    "freebsd": [
      "CC=gcc",
      "CXX=g++"
    ]
  },
  "dependency": [
    "huggingface_dataset",
    "opencv"
  ]
}
