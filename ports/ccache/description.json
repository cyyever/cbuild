{
  "git_url": "git@github.com:ccache/ccache.git",
  "main_language": "c",
  "environment_variable": [
    "run_test=0"
  ],
  "system_package_dependency": {
    "unix": [
      "gperf"
    ],
    "archlinux": [
      "hiredis"
    ],
    "fedora": [
      "libzstd-devel"
    ],
    "centos": [
      "libzstd-devel"
    ],
    "freebsd": [
      "zstd",
      "hiredis"
    ],
    "macos": [
      "zstd"
    ],
    "ubuntu": [
      "libhiredis-dev",
      "libzstd-dev",
      "libb2-dev"
    ]
  },
  "configure_options": "--disable-man"
}
