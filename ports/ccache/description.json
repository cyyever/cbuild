{
  "archlinux": {
    "group_package": true
  },
  "macos": {
    "group_package": true
  },
  "freebsd": {
    "group_package": true
  },
  "all_os": {
    "git_url": "git@github.com:ccache/ccache.git",
    "main_language": "c",
    "environment_variable": [
      "run_test=0"
    ],
    "cmake_options": "-DHIREDIS_FROM_INTERNET=on -DZSTD_FROM_INTERNET=on"
  },
  "system_package_dependency": {
    "unix": [
      "gperf"
    ],
    "freebsd": [
      "ccache"
    ],
    "archlinux": [
      "ccache"
    ]
  }
}
