{
  "freebsd": {
    "group_package": true
  },
  "macos": {
    "group_package": true
  },
  "all_os": {
    "git_url": "git@github.com:protocolbuffers/protobuf.git",
    "git_branch": "v23.0",
    "main_language": "c",
    "build_languages": [
      "c",
      "python"
    ],
    "cache_time": 999,
    "cmake_options": "-DABSL_PROPAGATE_CXX_STD=on -Dprotobuf_ABSL_PROVIDER=package -Dprotobuf_BUILD_TESTS=off -DBUILD_SHARED_LIBS=on",
    "environment_variable": [
      "py_pkg_name=protobuf",
      "run_test=0"
    ]
  },
  "system_package_dependency": {
    "freebsd": [
      "protobuf"
    ],
    "macos": [
      "protobuf"
    ]
  }
}
