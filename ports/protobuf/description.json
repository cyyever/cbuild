{
  "freebsd": {
    "group_package": true
  },
  "macos": {
    "group_package": true
  },
  "conditional_new_feature": {
    "unix": [
      "machine_learning_compilers"
    ]
  },
  "all_os": {
    "git_url": "git@github.com:protocolbuffers/protobuf.git",
    "main_language": "c",
    "build_languages": [
      "c",
      "python"
    ],
    "cache_time": 999,
    "cmake_options": "-Dprotobuf_ABSL_PROVIDER=module -Dprotobuf_BUILD_TESTS=off -DBUILD_SHARED_LIBS=on -Dprotobuf_USE_EXTERNAL_GTEST=off",
    "environment_variable": [
      "py_pkg_name=protobuf",
      "run_test=1"
    ]
  },
  "system_package_dependency": {
    "freebsd": [
      "protobuf"
    ],
    "macos": [
      "protobuf"
    ]
  }
}
