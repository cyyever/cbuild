{
  "git_url": "git@github.com:cyyever/pytorch.git",
  "reuse_build": true,
  "ignored_git_submodules": [
    "third_party/eigen",
    "third_party/aiter",
    "third_party/opentelemetry-cpp",
    "third_party/composable_kernel",
    "third_party/XNNPACK",
    "android/libs/fbjni"
  ],
  "conditional_ignored_git_submodules": {
    "freebsd": [
      "third_party/cudnn_frontend",
      "third_party/cutlass",
      "third_party/flash-attention"
    ]
  },
  "cache_time": 30,
  "new_feature": [
    "ml_lib",
    "use_mold"
  ],
  "main_language": "python",
  "environment_variable": [
    "run_test=0",
    "slow_static_analysis=0",
    "BUILD_TEST=off",
    "USE_SYSTEM_PYBIND11=off",
    "USE_NUMPY=on",
    "USE_GLOO=on",
    "USE_XPU=off",
    "USE_MAGMA=off",
    "ATEN_NO_TEST=ON",
    "CMAKE_CXX_STANDARD=17",
    "CMAKE_CUDA_STANDARD=17",
    "USE_DISTRIBUTED=on",
    "BUILD_CUSTOM_PROTOBUF=off",
    "USE_FBGEMM=on",
    "USE_SYSTEM_ONNX=off",
    "USE_KINETO=off",
    "USE_TENSORPIPE=off",
    "TP_BUILD_LIBUV=off",
    "USE_GLOG=off",
    "USE_FLASH_ATTENTION=off",
    "USE_GFLAGS=off",
    "ONNX_ML=on",
    "USE_XNNPACK=off",
    "USE_PYTORCH_QNNPACK=off",
    "BLAS=OpenBLAS",
    "WERROR=off",
    "CMAKE_POLICY_VERSION_MINIMUM=3.5",
    "OpenBLAS_HOME=${INSTALL_PREFIX}/include/openblas",
    "OpenBLAS=${INSTALL_PREFIX}"
  ],
  "conditional_environment_variable": {
    "cuda": [
      "USE_CUDA=on",
      "USE_CUDNN=on",
      "USE_NCCL=on",
      "USE_SYSTEM_NCCL=on",
      "NCCL_ROOT_DIR=~/.local/lib/python${PYTHONVERSION}/site-packages/nvidia/nccl",
      "USE_NATIVE_ARCH=on"
    ],
    "cuda&&!windows": [
      "CUSPARSELT_ROOT_DIR=~/.local/lib/python${PYTHONVERSION}/site-packages/nvidia/cusparselt"
    ],
    "docker": [
      "USE_STATIC_CUDNN=off",
      "USE_NCCL=on"
    ],
    "debug_build": [
      "DEBUG=1"
    ],
    "windows": [
      "BLAS=OpenBLAS",
      "USE_SYSTEM_PYBIND11=1",
      "BUILD_CUSTOM_PROTOBUF=on"
    ],
    "freebsd": [
      "CMAKE_GENERATOR='Unix Makefiles'",
      "USE_DISTRIBUTED=off",
      "USE_SYSTEM_GLOO=on",
      "USE_SYSTEM_ONNX=off",
      "DEBUG=1",
      "USE_FBGEMM=on"
    ],
    "macos": [
      "BLAS=vecLib",
      "BUILD_CUSTOM_PROTOBUF=on",
      "USE_FBGEMM=off",
      "USE_DISTRIBUTED=off"
    ]
  },
  "dependency": [
    "glog"
  ],
  "system_package_dependency": {
    "ubuntu": [
      "libnuma-dev",
      "libgtest-dev"
    ],
    "archlinux": [
      "gflags",
      "numactl"
    ],
    "pip_nodeps&&cuda": [
      "nvidia-nccl-cu13",
      "nvidia-cusparselt-cu13"
    ]
  }
}
