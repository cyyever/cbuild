{
  "git_url": "git@github.com:cyyever/pytorch_private.git",
  "remote_git_url": "git@github.com:pytorch/pytorch.git",
  "remote_git_branch": "master",
  "reuse_build": true,
  "ignored_git_submodules": [
    "third_party/eigen",
    "third_party/pybind11",
    "third_party/ios-cmake",
    "third_party/nccl/nccl",
    "third_party/python-enum",
    "third_party/python-six",
    "third_party/QNNPACK",
    "third_party/breakpad",
    "third_party/protobuf",
    "third_party/tbb"
  ],
  "cache_time": 30,
  "conditional_new_feature": {
    "support_cuda": [
      "cuda"
    ],
    "unix": [
      "machine_learning_compilers"
    ]
  },
  "main_language": "python",
  "environment_variable": [
    "run_test=0",
    "static_analysis=0",
    "clang_tidy_static_analysis=0",
    "clang_tidy_fix=0",
    "py_pkg_name=torch",
    "USE_GOLD_LINKER=on",
    "BUILD_TEST=off",
    "USE_MPI=OFF",
    "USE_SYSTEM_PYBIND11=on",
    "ATEN_NO_TEST=ON",
    "COLORIZE_OUTPUT=off",
    "BUILD_CAFFE2=off",
    "USE_SANITIZER=off",
    "USE_TSAN=off",
    "NO_VPTR=on",
    "USE_SYSTEM_BIND11=on",
    "BUILD_CAFFE2_MOBILE=off",
    "BUILD_CUSTOM_PROTOBUF=off",
    "USE_DISTRIBUTED=off",
    "USE_SYSTEM_EIGEN_INSTALL=ON",
    "USE_FBGEMM=on",
    "USE_NATIVE_ARCH=on",
    "USE_ROCM=off",
    "USE_BREAKPAD=off",
    "USE_KINETO=off",
    "USE_TENSORRT=off",
    "GLIBCXX_USE_CXX11_ABI=1",
    "USE_OPENCV=off",
    "USE_QNNPACK=off",
    "USE_PYTORCH_QNNPACK=off",
    "USE_METAL=off",
    "USE_NUMA=ON",
    "USE_FFMPEG=off",
    "USE_GLOG=off",
    "USE_GFLAGS=off",
    "ONNX_ML=off",
    "BLAS=OpenBLAS",
    "PYTHON_EXECUTABLE=${CBUILD_PYTHON_EXE}",
    "OpenBLAS_HOME=${INSTALL_PREFIX}/include/openblas",
    "OpenBLAS=${INSTALL_PREFIX}"
  ],
  "conditional_environment_variable": {
    "cuda": [
      "USE_CUDA=on",
      "USE_CUDNN=on",
      "USE_MKLDNN=off",
      "BUILD_SPLIT_CUDA=on",
      "CUDNN_ROOT=${INSTALL_PREFIX}/cudnn",
      "CUDNN_LIBRARY=${INSTALL_PREFIX}/cudnn"
    ],
    "freebsd": [
      "USE_MKLDNN=on"
    ],
    "debug_build": [
      "DEBUG=1"
    ],
    "windows": [
      "USE_OPENMP=off"
    ],
    "linux&&cuda": [
      "NCCL_ROOT=${INSTALL_PREFIX}",
      "NCCL_INCLUDE_DIR=${INSTALL_PREFIX}/include",
      "USE_NCCL=off",
      "USE_SYSTEM_NCCL=off"
    ]
  },
  "dependency": [
    "machine_learning_dev_env",
    "glog",
    "pybind11:master",
    "protobuf[shared_lib]"
  ],
  "system_package_dependency": {
    "ubuntu": [
      "libnuma-dev",
      "libgtest-dev"
    ],
    "pip": [
      "typing_extensions"
    ],
    "archlinux": [
      "gflags",
      "numactl"
    ]
  }
}
