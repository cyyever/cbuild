{
  "windows": {
    "group_package": true,
    "system_package_dependency": {
      "pip": [
        "torch"
      ]
    }
  },
  "all_os": {
    "git_url": "git@github.com:cyyever/pytorch_private.git",
    "remote_git_url": "git@github.com:pytorch/pytorch.git",
    "remote_git_branch": "master",
    "git_branch": "cyy",
    "reuse_build": true,
    "ignored_git_submodules": [
      "third_party/eigen",
      "third_party/pybind11",
      "third_party/ios-cmake",
      "third_party/nccl/nccl",
      "third_party/python-enum",
      "third_party/python-six",
      "third_party/QNNPACK",
      "third_party/breakpad",
      "third_party/protobuf",
      "third_party/tbb"
    ],
    "cache_time": 30,
    "conditional_new_feature": {
      "support_cuda": [
        "cuda"
      ],
      "unix&&!macos": [
        "use_mold"
      ],
      "unix": [
        "machine_learning_compilers"
      ]
    },
    "main_language": "python",
    "environment_variable": [
      "run_test=0",
      "static_analysis=0",
      "slow_static_analysis=0",
      "clang_tidy_fix=0",
      "py_pkg_name=torch",
      "BUILD_TEST=off",
      "BUILD_NVFUSER=off",
      "USE_MPI=OFF",
      "USE_SYSTEM_PYBIND11=on",
      "ATEN_NO_TEST=ON",
      "COLORIZE_OUTPUT=on",
      "BUILD_CAFFE2=off",
      "USE_MEMORY_SANITIZERS=off",
      "USE_MSAN=off",
      "USE_TSAN=off",
      "NO_VPTR=on",
      "USE_SYSTEM_BIND11=on",
      "BUILD_CUSTOM_PROTOBUF=off",
      "USE_DISTRIBUTED=on",
      "USE_GLOO=off",
      "USE_SYSTEM_EIGEN_INSTALL=ON",
      "USE_FBGEMM=on",
      "USE_NATIVE_ARCH=on",
      "USE_ROCM=off",
      "USE_BREAKPAD=off",
      "USE_KINETO=off",
      "USE_TENSORRT=off",
      "GLIBCXX_USE_CXX11_ABI=1",
      "USE_OPENCV=off",
      "USE_QNNPACK=off",
      "USE_PYTORCH_QNNPACK=off",
      "USE_METAL=off",
      "USE_NUMA=ON",
      "USE_FFMPEG=off",
      "USE_GLOG=off",
      "USE_GFLAGS=off",
      "ONNX_ML=off",
      "USE_MKLDNN=off",
      "BLAS=OpenBLAS",
      "PYTHON_EXECUTABLE=${CBUILD_PYTHON_EXE}",
      "OpenBLAS_HOME=${INSTALL_PREFIX}/include/openblas",
      "OpenBLAS=${INSTALL_PREFIX}"
    ],
    "conditional_environment_variable": {
      "cuda": [
        "USE_CUDA=on",
        "USE_CUDNN=on",
        "USE_MKLDNN=off",
        "CUDNN_ROOT=${INSTALL_PREFIX}/cudnn",
        "CUDNN_LIBRARY=${INSTALL_PREFIX}/cudnn"
      ],
      "amd64&&!cuda": [
        "USE_MKLDNN=on"
      ],
      "debug_build": [
        "DEBUG=1"
      ],
      "windows": [
        "USE_OPENMP=off"
      ],
      "linux&&cuda": [
        "NCCL_ROOT=${INSTALL_PREFIX}",
        "NCCL_INCLUDE_DIR=${INSTALL_PREFIX}/include",
        "USE_NCCL=on",
        "USE_SYSTEM_NCCL=on"
      ],
      "freebsd": [
        "USE_DISTRIBUTED=off"
      ]
    }
  },
  "dependency": [
    "machine_learning_dev_env",
    "glog",
    "pybind11",
    "protobuf"
  ],
  "system_package_dependency": {
    "ubuntu": [
      "libnuma-dev",
      "libgtest-dev"
    ],
    "pip": [
      "typing_extensions"
    ],
    "archlinux": [
      "gflags",
      "numactl"
    ]
  }
}
