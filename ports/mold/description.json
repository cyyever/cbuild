{
  "archlinux": {
    "group_package": true
  },
  "all_os": {
    "git_url": "git@github.com:rui314/mold.git",
    "main_language": "c",
    "reuse_build": true,
    "cache_time": 999,
    "environment_variable": [
      "no_install=1",
      "SYSTEM_TBB=1",
      "run_test=0"
    ],
    "cmake_options": "-DMOLD_USE_SYSTEM_MIMALLOC=on",
    "dependency": [
      "mimalloc"
    ]
  },
  "system_package_dependency": {
    "archlinux": [
      "mold"
    ]
  }
}
