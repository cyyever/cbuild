{
  "archlinux": {
    "group_package": true
  },
  "freebsd": {
    "group_package": true
  },
  "all_os": {
    "git_url": "git@github.com:gflags/gflags.git",
    "main_language": "c",
    "conditional_environment_variable": {
      "freebsd": [
        "run_test=0"
      ]
    }
  },
  "system_package_dependency": {
    "freebsd": [
      "gflags"
    ],
    "archlinux": [
      "gflags"
    ]
  }
}
