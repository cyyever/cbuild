{
  "archlinux": {
    "group_package": true,
    "system_package_dependency": {
      "archlinux": [
        "gflags"
      ]
    }
  },
  "freebsd": {
    "group_package": true,
    "system_package_dependency": {
      "freebsd": [
        "gflags"
      ]
    }
  },
  "all_os": {
    "git_url": "git@github.com:gflags/gflags.git",
    "main_language": "c",
    "conditional_environment_variable": {
      "freebsd": [
        "run_test=0"
      ]
    }
  }
}
