{
  "git_url": "git@github.com:grpc/grpc.git",
  "main_language": "c",
  "reuse_build": true,
  "cmake_options": "-DBUILD_TESTING=OFF -DgRPC_SSL_PROVIDER=package -DgRPC_ZLIB_PROVIDER=package -DgRPC_PROTOBUF_PROVIDER=package -DgRPC_BUILD_GRPC_PYTHON_PLUGIN=on",
  "ignored_git_submodules": [
    "third_party/protobuf",
    "third_party/boringssl-with-bazel",
    "third_party/zlib"
  ],
  "conditional_dependency": {
    "windows": [
      "protobuf[shared_lib]:master",
      "openssl[shared_lib]:__cbuild_most_recent_git_tag"
    ],
    "!windows": [
      "protobuf[shared_lib]:__cbuild_most_recent_git_tag"
    ]
  }
}
