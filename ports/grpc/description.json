{
  "git_url": "git@github.com:grpc/grpc.git",
  "default_build_script": "c",
  "reuse_build": true,
  "cmake_options": "-DBUILD_TESTING=OFF -DgRPC_SSL_PROVIDER=package -DgRPC_ZLIB_PROVIDER=package -DgRPC_PROTOBUF_PROVIDER=package",
  "ignored_git_submodules": [
    "third_party/protobuf",
    "third_party/boringssl-with-bazel",
    "third_party/zlib"
  ],
  "dependency": [
    "protobuf:__cbuild_most_recent_git_tag"
  ]
}
