{
  "freebsd": {
    "group_package": true
  },
  "archlinux": {
    "group_package": true
  },
  "ubuntu": {
    "group_package": true
  },
  "macos": {
    "group_package": true
  },
  "all_os": {
    "git_url": "git@github.com:glennrp/libpng.git",
    "main_language": "c",
    "environment_variable": [
      "run_test=1"
    ],
    "dependency": [
      "zlib-ng"
    ]
  },
  "system_package_dependency": {
    "freebsd": [
      "png"
    ],
    "macos": [
      "libpng"
    ],
    "archlinux": [
      "libpng"
    ],
    "ubuntu": [
      "libpng-dev"
    ]
  }
}
