{
  "archlinux": {
    "group_package": true
  },
  "macos": {
    "group_package": true
  },
  "all_os": {
    "git_url": "git@github.com:fish-shell/fish-shell.git",
    "main_language": "c",
    "new_feature": [
      "static_lib"
    ],
    "environment_variable": [
      "INSTALL_SUBDIR=fish-shell",
      "run_test=0"
    ],
    "cmake_options": "-DBUILD_DOCS=off"
  },
  "system_package_dependency": {
    "centos": [
      "ncurses-devel"
    ],
    "macos": [
      "fish"
    ],
    "archlinux": [
      "fish"
    ],
    "ubuntu": [
      "libncurses-dev"
    ]
  }
}
