{
  "git_url": "git@github.com:kovidgoyal/kitty.git",
  "environment_variable": [
    "LDFLAGS=-L/usr/local/lib",
    "static_analysis=1"
  ],
  "build_languages": [
    "python"
  ],
  "system_package_dependency": {
    "ubuntu": [
      "liblcms2-dev",
      "libxkbcommon-x11-dev",
      "libharfbuzz-dev",
      "libx11-xcb-dev"
    ],
    "freebsd": [
      "xorg",
      "dbus",
      "ncurses",
      "x11/startup-notification",
      "harfbuzz",
      "lcms2",
      "libxkbcommon",
      "wayland-protocols"
    ],
    "archlinux": [
      "lcms2",
      "mesa",
      "libxkbcommon-x11"
    ],
    "pip": [
      "sphinx"
    ],
    "fedora": [
      "harfbuzz-devel",
      "lcms2-devel",
      "fontconfig-devel",
      "mesa-libGL-devel",
      "libxkbcommon-devel",
      "libXrandr-devel",
      "libXinerama-devel",
      "libXcursor-devel",
      "libxkbcommon-x11-devel",
      "dbus-devel",
      "libXi-devel"
    ],
    "centos": [
      "lcms2-devel",
      "fontconfig-devel",
      "mesa-libGL-devel",
      "libxkbcommon-devel",
      "libXrandr-devel",
      "libXinerama-devel",
      "libXcursor-devel",
      "libxkbcommon-x11-devel",
      "dbus-devel",
      "libXi-devel"
    ],
    "unix": [
      "rsync"
    ]
  },
  "dependency": [
    "harfbuzz[shared_lib]"
  ]
}
