{
  "git_url": "git@github.com:rizsotto/Bear.git",
  "main_language": "c",
  "environment_variable": [
    "run_test=0",
    "CC=/usr/bin/cc",
    "CXX=/usr/bin/c++"
  ],
  "cmake_options": "-DENABLE_UNIT_TESTS=off -DENABLE_FUNC_TESTS=off",
  "system_package_dependency": {
    "archlinux": [
      "grpc"
    ]
  },
  "dependency": [
    "spdlog[shared_lib]",
    "fmt[shared_lib]",
    "nlohmann_json"
  ]
}
