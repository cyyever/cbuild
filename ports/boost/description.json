{
  "archlinux": {
    "group_package": true
  },
  "macos": {
    "group_package": true
  },
  "ubuntu": {
    "group_package": true
  },
  "all_os": {
    "git_url": "git@github.com:boostorg/boost.git",
    "git_branch": "boost-1.79.0",
    "main_language": "c",
    "environment_variable": [
      "run_test=0"
    ],
    "cmake_options": "-DBOOST_ENABLE_CMAKE=on",
    "cache_time": 300
  },
  "system_package_dependency": {
    "ubuntu": [
      "libboost-all-dev"
    ],
    "archlinux": [
      "boost"
    ],
    "macos": [
      "boost"
    ]
  }
}
