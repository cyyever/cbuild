{
  "git_url": "git@github.com:pytorch/vision.git",
  "git_branch": "main",
  "main_language": "python",
  "environment_variable": [
    "run_test=0",
    "static_analysis=1",
    "clang_tidy_static_analysis=1",
    "clang_tidy_fix=1",
    "TORCHVISION_INCLUDE=${INSTALL_PREFIX}/include"
  ],
  "conditional_new_feature": {
    "unix": [
      "machine_learning_compilers"
    ]
  },
  "conditional_environment_variable": {
    "unix": [
      "CPLUS_INCLUDE_PATH=${INSTALL_PREFIX}/oneTBB/include",
      "TORCHVISION_LIBRARY=${INSTALL_PREFIX}/lib:${INSTALL_PREFIX}/lib64"
    ],
    "windows": [
      "DISTUTILS_USE_SDK=0",
      "TORCHVISION_LIBRARY=${INSTALL_PREFIX}/lib;${INSTALL_PREFIX}/lib64"
    ],
    "ubuntu": [
      "NVCC_FLAGS=-ccbin ${CUDA_HOST_COMPILER}"
    ]
  },
  "dependency": [
    "pytorch:cyy"
  ]
}
