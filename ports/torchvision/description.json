{
  "git_url": "git@github.com:pytorch/vision.git",
  "default_build_script": "python",
  "environment_variable": [
    "run_test=0",
    "py_pkg_name=torchvision"
  ],
  "conditional_environment_variable": {
    "unix": [
      "CPLUS_INCLUDE_PATH=${INSTALL_PREFIX}/tbb/include"
    ]
  },
  "dependency": [
    "pytorch:master"
  ]
}
