{
  "git_url": "git@github.com:cyyever/algorithm.git",
  "main_language": "c",
  "dependency": [
    "doctest",
    "range-v3:master",
    "cyy_naive_cpp_lib[shared_lib]:master",
    "cyy_math[shared_lib]:master"
  ],
  "environment_variable": [
    "static_analysis=1",
    "clang_tidy_static_analysis=1"
  ],
  "conditional_cmake_options": {
    "python_binding": "-DBUILD_PYTHON_BINDING=on",
    "ubuntu": "-DCMAKE_CXX_COMPILER=g++-11"
  },
  "dependency": [
    "boost"
  ],
  "context_dependency": {
    "python_binding": [
      "pybind11:master"
    ]
  }
}
