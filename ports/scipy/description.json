{
  "git_url": "git@github.com:scipy/scipy.git",
  "cache_time": 30,
  "main_language": "python",
  "dependency": [
    "numpy",
    "pybind11"
  ],
  "environment_variable": [
    "run_test=0"
  ],
  "conditional_environment_variable": {
    "freebsd": [
      "FC=gfortran11"
    ]
  },
  "conditional_new_feature": {
    "unix": [
      "use_mold"
    ]
  },
  "system_package_dependency": {
    "pip": [
      "pythran"
    ]
  }
}
