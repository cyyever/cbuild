{
  "git_url": "git@github.com:FFmpeg/FFmpeg.git",
  "cache_time": 300,
  "dependency": [
    "assembly_env"
  ],
  "conditional_dependency": {
    "cuda": [
      "nv-codec-headers"
    ],
    "centos": [
      "x264[shared_lib]:master",
      "x265[shared_lib]"
    ],
    "mingw64": [
      "x264[shared_lib]:master",
      "x265[shared_lib]"
    ]
  },
  "environment_variable": [
    "run_test=1"
  ],
  "use_mingw64": true,
  "system_package_dependency": {
    "ubuntu": [
      "libx264-dev",
      "libx265-dev"
    ],
    "archlinux": [
      "x264",
      "x265"
    ],
    "fedora": [
      "openh264-devel"
    ],
    "freebsd": [
      "libx264",
      "x265"
    ]
  },
  "main_language": "c",
  "configure_options": "--extra-cflags=-fPIE --disable-doc --enable-nonfree --enable-gpl --enable-libx264 --enable-libx265",
  "conditional_configure_options": {
    "cuda": [
      " --enable-cuvid --enable-ffnvcodec"
    ]
  },
  "feature_configure_options": {
    "shared_lib": "--enable-shared"
  }
}
