{
  "git_url": "git@github.com:cyyever/naive_cpp_lib.git",
  "cache_time": 0,
  "default_build_script": "c",
  "conditional_new_feature": {
    "all": [
      "cv",
      "torch_python_binding",
      "cv_python_binding",
      "video_python_binding"
    ],
    "video": [
      "cv"
    ],
    "cv_python_binding": [
      "cv"
    ],
    "torch_python_binding": [
      "torch"
    ],
    "video_python_binding": [
      "video"
    ],
    "support_cuda": [
      "cuda"
    ]
  },
  "environment_variable": [
    "static_analysis=1"
  ],
  "dependency": [
    "doctest:__cbuild_most_recent_git_tag",
    "spdlog:__cbuild_most_recent_git_tag",
    "ms-gsl:__cbuild_most_recent_git_tag"
  ],
  "conditional_dependency": {
    "cv": [
      "opencv[full]:__cbuild_most_recent_git_tag"
    ],
    "torch": [
      "torchvision:master"
    ],
    "cv_python_binding": [
      "pybind11:__cbuild_most_recent_git_tag"
    ],
    "torch_python_binding": [
      "pybind11:__cbuild_most_recent_git_tag"
    ],
    "video_python_binding": [
      "pybind11:__cbuild_most_recent_git_tag"
    ],
    "video&&unix": [
      "ffmpeg[shared_lib]:__cbuild_most_recent_git_tag"
    ],
    "video&&windows": [
      "ffmpeg_windows:__cbuild_most_recent_git_tag"
    ],
    "cv&&cuda": [
      "cyy_cuda_buddy[shared_lib]:master"
    ]
  },
  "cmake_options": "-DDISABLE_RUNTIME_ANALYSIS=on",
  "conditional_cmake_options": {
    "ubuntu": "-DCMAKE_CXX_COMPILER=g++-10",
    "freebsd": "-DCMAKE_CXX_COMPILER=g++",
    "all": "-DBUILD_ALL=on",
    "torch": "-DBUILD_TORCH=on",
    "video": "-DBUILD_VIDEO=on",
    "cv": "-DBUILD_CV=on",
    "torch_python_binding": "-DBUILD_TORCH_PYTHON_BINDING=on",
    "cv_python_binding": "-DBUILD_CV_PYTHON_BINDING=on",
    "video_python_binding": "-DBUILD_VIDEO_PYTHON_BINDING=on"
  }
}
