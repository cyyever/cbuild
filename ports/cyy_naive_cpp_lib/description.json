{
  "git_url": "git@github.com:cyyever/naive_cpp_lib.git",
  "cache_time": 0,
  "reuse_build": true,
  "main_language": "c",
  "conditional_new_feature": {
    "all": [
      "cv",
      "machine_learning_compilers",
      "cv_python_binding",
      "video_python_binding"
    ],
    "video": [
      "cv"
    ],
    "cv_python_binding": [
      "cv"
    ],
    "video_python_binding": [
      "video"
    ],
    "support_cuda": [
      "cuda"
    ]
  },
  "environment_variable": [
    "static_analysis=0",
    "clang_tidy_static_analysis=1",
    "clang_tidy_fix=0"
  ],
  "dependency": [
    "doctest",
    "spdlog",
    "ms-gsl"
  ],
  "conditional_dependency": {
    "bsd": [
      "jthread:master"
    ],
    "cv": [
      "opencv[full]:cyy"
    ],
    "cv_python_binding": [
      "pybind11"
    ],
    "video_python_binding": [
      "pybind11"
    ],
    "video": [
      "ffmpeg[shared_lib]"
    ]
  },
  "conditional_cmake_options": {
    "all": "-DBUILD_ALL=on",
    "video": "-DBUILD_VIDEO=on",
    "cv": "-DBUILD_CV=on",
    "cv_python_binding": "-DBUILD_CV_PYTHON_BINDING=on",
    "video_python_binding": "-DBUILD_VIDEO_PYTHON_BINDING=on",
    "bsd": "-DBUILD_FUZZING=on",
    "windows": "-DBUILD_FUZZING=on"
  }
}
