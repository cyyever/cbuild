{
  "git_url": "git@github.com:uploadcare/pillow-simd.git",
  "cache_time": 30,
  "main_language": "python",
  "environment_variable": [
    "JPEG_ROOT=${INSTALL_PREFIX}",
    "py_pkg_name=pillow-simd",
    "CFLAGS=-mavx2",
    "run_test=0"
  ],
  "ignored_tag_regex": [
    "simd",
    "post"
  ],
  "dependency": [
    "libjpeg-turbo",
    "openjpeg"
  ]
}
