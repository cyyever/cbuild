{
  "git_url": "git@github.com:uploadcare/pillow-simd.git",
  "cache_time": 30,
  "default_build_script": "python",
  "environment_variable": [
    "run_test=0",
    "JPEG_ROOT=${INSTALL_PREFIX}",
    "py_pkg_name=Pillow",
    "CFLAGS=-mavx2"
  ],
  "ignored_tag_regex": [
    "cmyk16bit"
  ],
  "dependency": [
    "libjpeg-turbo:__cbuild_most_recent_git_tag"
  ]
}
