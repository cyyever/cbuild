{
  "git_url": "git@github.com:pytorch/audio.git",
  "default_build_script": "python",
  "environment_variable": [
    "run_test=0",
    "BUILD_SOX=ON",
    "BUILD_KALDI=OFF",
    "BUILD_CUDA=ON"
  ],
  "ignored_git_submodules": [
    "kaldi"
  ],
  "dependency": [
    "pytorch:cyy"
  ],
  "system_package_dependency": {
    "ubuntu": [
      "libgsm1-dev"
    ],
    "pip": [
      "git+https://github.com/librosa/librosa.git@main"
    ]
  }
}
