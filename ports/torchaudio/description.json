{
  "git_url": "git@github.com:pytorch/audio.git",
  "git_branch": "main",
  "reuse_build": true,
  "main_language": "python",
  "environment_variable": [
    "run_test=0",
    "BUILD_SOX=ON",
    "BUILD_KALDI=OFF",
    "BUILD_CUDA=ON"
  ],
  "dependency": [
    "pytorch",
    "ffmpeg"
  ],
  "new_feature": [
    "ml_lib"
  ],
  "system_package_dependency": {
    "ubuntu": [
      "libgsm1-dev"
    ],
    "pip": [
      "librosa"
    ]
  }
}
