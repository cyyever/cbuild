{
  "git_url": "git@github.com:pybind/pybind11.git",
  "cache_time": 30,
  "reuse_build": true,
  "main_language": "c",
  "build_languages": [
    "c",
    "python"
  ],
  "environment_variable": [
    "run_test=0",
    "TEST_TARGET=check"
  ],
  "conditional_environment_variable": {
    "windows": [
      "run_test=0"
    ],
    "unix": [
      "run_test=1",
      "slow_static_analysis=1",
      "clang_tidy_fix=1",
      "static_analysis=1"
    ]
  },
  "cmake_options": "-DPYBIND11_TEST=on -DDOWNLOAD_CATCH=ON -DCMAKE_CXX_STANDARD=14"
}
