{
  "git_url": "git@github.com:cyyever/pybind11.git",
  "remote_git_url": "git@github.com:pybind/pybind11.git",
  "remote_git_branch": "master",
  "git_branch": "cyy",
  "cache_time": 30,
  "reuse_build": true,
  "main_language": "c",
  "build_languages": [
    "c",
    "python"
  ],
  "environment_variable": [
    "static_analysis=1",
    "slow_static_analysis=1",
    "clang_tidy_fix=1",
    "run_test=1",
    "TEST_TARGET=check"
  ],
  "conditional_environment_variable": {
    "freebsd": [
      "run_test=0"
    ]
  },
  "cmake_options": "-DPYBIND11_TEST=on -DDOWNLOAD_CATCH=ON",
  "dependency": [
    "boost"
  ]
}
