{
  "git_url": "git@github.com:cyyever/vllm.git",
  "cache_time": 30,
  "main_language": "python",
  "new_feature": [
    "ml_lib",
    "use_mold"
  ],
  "environment_variable": [
    "CUTLASS_NVCC_ARCHS=${CUDAARCHS}",
    "CUTLASS_NVCC_ARCHS_ENABLED=${CUDAARCHS}",
    "FLASH_ATTENTION_FORCE_CXX11_ABI=TRUE",
    "TORCH_CUDA_ARCH_LIST=${CUDAARCHS}",
    "FLASH_ATTN_CUDA_ARCHS=${CUDAARCHS}",
    "MAX_JOBS=20",
    "run_test=0",
    "use_setup_py=1"
  ],
  "system_package_dependency": {
    "pip": [
      "setuptools_scm"
    ]
  }
}
