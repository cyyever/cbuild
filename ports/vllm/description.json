{
  "git_url": "git@github.com:vllm-project/vllm.git",
  "cache_time": 30,
  "main_language": "python",
  "new_feature": [
    "ml_lib",
    "use_mold"
  ],
  "environment_variable": [
    "MAX_JOBS=50",
    "run_test=0",
    "use_setup_py=1",
    "VLLM_FLASH_ATTN_SRC_DIR=${SRC_DIR}/../flash-attention"
  ],
  "dependency": [
    "torchvision",
    "vllm-flash-attention"
  ],
  "system_package_dependency": {
    "pip": [
      "setuptools_scm"
    ]
  }
}
