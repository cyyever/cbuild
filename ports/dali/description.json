{
  "git_url": "git@github.com:cyyever/DALI.git",
  "remote_git_url": "git@github.com:NVIDIA/DALI.git",
  "remote_git_branch": "main",
  "cache_time": 30,
  "default_build_script": "c",
  "reuse_build": true,
  "environment_variable": [
    "run_test=0",
    "py_pkg_name=nvidia-dali-cuda110",
    "static_analysis=0",
    "clang_tidy_static_analysis=0",
    "pvs_static_analysis=1",
    "clang_tidy_fix=0"
  ],
  "conditional_new_feature": {
    "unix": [
      "machine_learning_compilers"
    ]
  },
  "conditional_dependency": {
    "!docker": [
      "dali_extra:main"
    ]
  },
  "dependency": [
    "torchvision:cyy",
    "ffmpeg[shared_lib]:__cbuild_most_recent_git_tag",
    "opencv[full]:__cbuild_most_recent_git_tag",
    "libtiff:__cbuild_most_recent_git_tag"
  ],
  "ignored_git_submodules": [
    "third_party/spdlog",
    "third_party/pybind11"
  ],
  "cmake_options": "-DFFMPEG_ROOT_DIR=${INSTALL_PREFIX} -DCUDA_ARCHITECTURES='75' -DCUDA_TARGET_ARCHS='75' -DBUILD_TEST=on -DBUILD_BENCHMARK=off -DBUILD_LIBTIFF=on -DBUILD_LIBSND=off -DProtobuf_USE_STATIC_LIBS=off -DBUILD_OPENCV=off -DCMAKE_CUDA_ARCHITECTURES='75'",
  "system_package_dependency": {
    "ubuntu": [
      "libsndfile1-dev",
      "libclang-dev"
    ],
    "pip": [
      "clang"
    ]
  }
}
