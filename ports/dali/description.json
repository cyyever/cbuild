{
  "git_url": "git@github.com:NVIDIA/DALI.git",
  "cache_time": 30,
  "default_build_script": "c",
  "dependency": [
    "torchvision:cyy",
    "ffmpeg[shared_lib]:__cbuild_most_recent_git_tag",
    "opencv[full]:__cbuild_most_recent_git_tag"
  ],
  "cmake_options": "-DFFMPEG_ROOT_DIR=${INSTALL_PREFIX} -DCUDA_TARGET_ARCHS='70;75;80' -DProtobuf_USE_STATIC_LIBS=off -DBUILD_TEST=off -DBUILD_BENCHMARK=off",
  "system_package_dependency": {
    "ubuntu": [
      "libsndfile1-dev",
      "libclang-11-dev"
    ],
    "pip": [
      "clang"
    ]
  }
}
