{
  "git_url": "git@github.com:numpy/numpy.git",
  "cache_time": 30,
  "default_build_script": "python",
  "dependency": [
    "openblas[shared_lib]:__cbuild_most_recent_git_tag"
  ],
  "environment_variable": [
    "run_test=1",
    "static_analysis=1",
    "use_bear=1",
    "pvs_static_analysis=1",
    "clang_tidy_static_analysis=1"
  ],
  "system_package_dependency": {
    "pip": [
      "hypothesis",
      "Cython"
    ]
  }
}
