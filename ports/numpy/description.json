{
  "git_url": "git@github.com:numpy/numpy.git",
  "cache_time": 30,
  "main_language": "python",
  "conditional_dependency": {
    "!linux": [
      "openblas[shared_lib]:cmake_fix"
    ],
    "linux": [
      "openblas_libtool[shared_lib]"
    ]
  },
  "environment_variable": [
    "run_test=1",
    "static_analysis=0",
    "need_compilation_json=1",
    "clang_tidy_static_analysis=1"
  ],
  "conditional_environment_variable": {
    "freebsd": [
      "CC=gcc",
      "CXX=g++"
    ]
  },
  "system_package_dependency": {
    "pip": [
      "hypothesis",
      "Cython"
    ]
  }
}
