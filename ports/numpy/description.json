{
  "git_url": "git@github.com:numpy/numpy.git",
  "cache_time": 30,
  "main_language": "python",
  "dependency": [
    "openblas[shared_lib]"
  ],
  "environment_variable": [
    "run_test=1",
    "static_analysis=0",
    "need_compilation_json=1",
    "clang_tidy_static_analysis=1",
    "NPY_NUM_BUILD_JOBS=6"
  ],
  "conditional_environment_variable": {
    "windows": [
      "PYTHON_BUILD_CMD=${CBUILD_PYTHON_EXE} setup.py config --fcompiler=intelem build_ext --inplace"
    ]
  },
  "conditional_new_feature": {
    "unix&&!macos": [
      "use_mold"
    ]
  },
  "system_package_dependency": {
    "pip": [
      "hypothesis",
      "Cython"
    ]
  }
}
