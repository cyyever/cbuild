{
  "conditional_dependency": {
    "feature_language_c": [
      "cxx_env:master"
    ],
    "feature_language_cpp": [
      "cxx_env:master"
    ],
    "feature_language_golang": [
      "golang_env:master"
    ],
    "feature_language_nodejs": [
      "nodejs_env:master"
    ],
    "feature_language_haskell": [
      "haskell_env:master"
    ],
    "feature_language_python": [
      "python_env:master"
    ],
    "feature_language_java": [
      "java_env:master"
    ],
    "feature_language_rust": [
      "rust_env:master"
    ],
    "feature_building_tool_cmake": [
      "cmake[static_lib]"
    ],
    "feature_package_manager_yay": [
      "yay:master"
    ],
    "use_mold": [
      "mold:main"
    ],
    "cuda&&!docker": [
      "CUDA"
    ],
    "mingw64": [
      "mingw64_env"
    ],
    "all_os": [
      "OS:main"
    ]
  },
  "conditional_system_package_manager": {
    "ubuntu": [
      "apt"
    ],
    "freebsd": [
      "pkg",
      "bsd_ports"
    ],
    "macos": [
      "brew"
    ],
    "centos": [
      "yum"
    ],
    "fedora": [
      "yum"
    ],
    "windows": [
      "chocolatey",
      "msys"
    ],
    "mingw64": [
      "mingw64"
    ],
    "archlinux": [
      "pacman",
      "yay"
    ]
  },
  "conditional_cmake_options": {
    "all_os": "-DCMAKE_EXPORT_COMPILE_COMMANDS=on -DCMAKE_PREFIX_PATH=${INSTALL_PREFIX} -DCMAKE_BUILD_TYPE=${BUILD_TYPE} -DBUILD_SHARED_LIBS=on",
    "shared_lib": "-DBUILD_SHARED_LIBS=on",
    "static_lib": "-DBUILD_SHARED_LIBS=off",
    "unix": "-DPYTHON_EXECUTABLE=${CBUILD_PYTHON_EXE} -DCUDA_TOOLKIT_ROOT_DIR=${CUDA_HOME} -DCMAKE_IGNORE_PATH=/usr/local/cuda/lib64",
    "machine_learning_compilers": "-DCUDA_HOST_COMPILER=${CUDA_HOST_COMPILER} -DCMAKE_CUDA_HOST_COMPILER=${CUDA_HOST_COMPILER}"
  },
  "conditional_docker_base_image": {
    "all_os": "ubuntu:devel"
  },
  "cache_time": 10
}
